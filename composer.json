{
  "name": "mledoze/countries",
  "version": "3.0.0",
  "description": "List of world countries in JSON, CSV, XML and Yaml",
  "keywords": ["world", "countries", "json", "csv", "xml", "yaml"],
  "homepage": "https://mledoze.github.io/countries/",
  "license": "ODbL-1.0",
  "authors": [
	{"name": "Mohammed Le Doze"}
  ],
  "repositories": [
	{
	  "type": "vcs",
	  "url": "https://github.com/mledoze/countries"
	}
  ],
  "require": {
	"php": ">=7.2",
	"symfony/console": "^4.1",
	"symfony/yaml": "^4.1",
	"ext-gettext": "*"
  },
  "require-dev": {
	"roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8"
  },
  "autoload": {
	"psr-4": {"MLD\\": "src/MLD"}
  },
  "bin": ["countries.php"],
  "scripts": {
	"run": "@php countries.php convert",
	"test": "phpunit"
  }
}
